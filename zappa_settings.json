{
    "prod": {
        "app_function": "mercado_bitcoin.lambda_function",
        "aws_region": "us-east-1",
        "profile_name": "rick_aws",
        "project_name": "mercado-bitcoin-rick-lambda",
        "runtime": "python3.8",
        "s3_bucket": "zappa-ricks-deployments",
        "enviroment_variables": {"STAGE": "prod"},
        "lambda_handler": "mercado_bitcoin.lambda_function.lambda_handler",
        "events": [
            {
                "function": "mercado_bitcoin.lambda_function.lambda_handler",
                "expression": "rate(1 minute)"
            }
        ],
        "memory_size": 128,
        "apigateway_enabled": false
    },
    "dev": {
        "extends": "prod",
        "debug": true,
        "enviroment_variables": {"STAGE": "dev"},
        "events": [
            {
                "function": "mercado_bitcoin.lambda_function.lambda_handler",
                "expression": "rate(60 minute)"
            }
        ]
   }
}